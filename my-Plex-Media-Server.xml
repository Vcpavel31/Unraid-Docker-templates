    <Volume>
      <HostDir>/mnt/user/appdata/Plex-Media-Server</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>claim-dFHz57oytKvAogDNxtQH</Value>
      <Name>PLEX_CLAIM</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>99</Value>
      <Name>PLEX_UID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PLEX_GID</Name>
