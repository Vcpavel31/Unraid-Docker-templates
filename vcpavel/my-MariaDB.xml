<?xml version="1.0"?>
<Container version="2">
  <Name>MariaDB</Name>
  <Repository>mariadb</Repository>
  <Registry>https://hub.docker.com/_/mariadb</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>true</Privileged>
  <Support/>
  <Project/>
  <Overview>MariaDB is a community-developed fork of MySQL intended to remain free under the GNU GPL.&#xD;
</Overview>
  <Category/>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://d1q6f0aelx0por.cloudfront.net/product-logos/library-mariadb-logo.png</Icon>
  <ExtraParams>--restart=unless-stopped</ExtraParams>
  <PostArgs/>
  <CPUset>0,1</CPUset>
  <DateInstalled>1609010304</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>MariaDB is a community-developed fork of MySQL intended to remain free under the GNU GPL.&#xD;
</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>3306</HostPort>
        <ContainerPort>3306</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/MariaDB/mysql</HostDir>
      <ContainerDir>/var/lib/mysql</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>Pavel31213</Value>
      <Name>MYSQL_ROOT_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>Europe/Prague</Value>
      <Name>TZ</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>Pavels_House</Value>
      <Name>MYSQL_DATABASE</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Root_Password" Target="MYSQL_ROOT_PASSWORD" Default="" Mode="" Description="Container Variable: MYSQL_ROOT_PASSWORD" Type="Variable" Display="always" Required="false" Mask="false">Pavel31213</Config>
  <Config Name="Time Zone" Target="TZ" Default="" Mode="" Description="Container Variable: TZ" Type="Variable" Display="always" Required="false" Mask="false">Europe/Prague</Config>
  <Config Name="Database" Target="MYSQL_DATABASE" Default="" Mode="" Description="Container Variable: MYSQL_DATABASE" Type="Variable" Display="always" Required="false" Mask="false">Pavels_House</Config>
  <Config Name="Port" Target="3306" Default="" Mode="tcp" Description="Container Port: 3306" Type="Port" Display="always" Required="false" Mask="false">3306</Config>
  <Config Name="Host Path 1" Target="/var/lib/mysql" Default="" Mode="rw" Description="Container Path: /var/lib/mysql" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/MariaDB/mysql</Config>
</Container>